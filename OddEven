import java.util.Scanner;

class Main
{
public static void main(String args[])
	{
	int num;
	System.out.println("Enter any Number");

	Scanner input = new Scanner(System.in);
	num = input.nextInt();

	if(num%2==0)
	  {
	  System.out.println("Entered number is ("+num+"), and  is EVEN");
	  }
	else
	    {
	     System.out.println("Entered number is ("+num+"), and is  ODD");
 	    }
	}
}



